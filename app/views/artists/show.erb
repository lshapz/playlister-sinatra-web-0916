<h1><%= @artist.name %></h1>
<h3>Songs
<ul><% @artist.songs.each do |song| %>
<p><li><a href="/songs/<%=song.slug%>"><%= song.name %></a></li></p>
<% end %></ul>  
<h3>Genres
<ul><% @artist.genres.uniq.each do |genre| %>
<p><li><a href="/genres/<%=genre.slug%>"><%= genre.name.capitalize %></a></li></p>
<% end %></ul></h3>


